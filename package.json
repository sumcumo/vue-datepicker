{
  "name": "@sum.cumo/vue-datepicker",
  "version": "4.0.0",
  "description": "A simple, but powerful, Vue.js datepicker component. Supports disabling of dates, inline mode, translations & custom slots",
  "license": "Apache-2.0",
  "homepage": "https://github.com/sumcumo/vue-datepicker",
  "repository": {
    "type": "git",
    "url": "git@github.com:sumcumo/vue-datepicker.git"
  },
  "author": "sum.cumo GmbH",
  "keywords": [
    "vue",
    "datepicker",
    "date-picker",
    "calendar",
    "keyboard navigation",
    "typeable"
  ],
  "main": "dist/Datepicker.common.js",
  "module": "dist/Datepicker.esm.js",
  "unpkg": "dist/Datepicker.min.js",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "src",
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "npm run clear && npm run build",
    "build": "npm run build:app && npm run build:locale",
    "build:app": "rollup -c ./scripts/build.js",
    "build:locale": "babel-node scripts/build-locale.js",
    "clear": "rm -rf dist/*",
    "audit": "npm audit --registry=https://registry.npmjs.org",
    "serve": "vite serve ./example",
    "serve:e2e": "vite serve ./example-e2e --port=8080",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "npx cypress open",
    "test:e2e:ci": "npx cypress run --spec **/*.feature",
    "release": "standard-version",
    "prerelease": "standard-version --dry-run",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "prettier": "./prettier.js",
  "peerDependencies": {
    "vue": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/eslint-parser": "7.19.1",
    "@babel/node": "7.19.1",
    "@babel/plugin-transform-runtime": "7.19.1",
    "@babel/preset-env": "7.19.4",
    "@rollup/plugin-alias": "4.0.0",
    "@rollup/plugin-babel": "6.0.0",
    "@rollup/plugin-commonjs": "23.0.0",
    "@rollup/plugin-node-resolve": "15.0.0",
    "@rollup/plugin-replace": "5.0.0",
    "@vitejs/plugin-vue2": "1.1.2",
    "@vitest/coverage-c8": "0.24.3",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/test-utils": "1.3.0",
    "autoprefixer": "10.4.12",
    "babel-core": "7.0.0-bridge.0",
    "chalk": "4.1.2",
    "cleave.js": "1.6.0",
    "core-js": "3.25.5",
    "cypress": "10.10.0",
    "cypress-cucumber-preprocessor": "4.3.1",
    "cypress-plugin-tab": "1.0.5",
    "date-fns": "2.29.3",
    "eslint": "8.25.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vitest": "0.0.8",
    "eslint-plugin-vue": "9.6.0",
    "jsdom": "20.0.1",
    "pre-commit": "1.2.2",
    "prettier": "2.7.1",
    "rollup": "2.79.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-vue": "6.0.0",
    "sass": "1.55.0",
    "semver": "7.3.8",
    "standard-version": "9.5.0",
    "vee-validate": "2.2.15",
    "vite": "3.1.8",
    "vitest": "0.24.3",
    "vue": "2.7.13",
    "vue-server-renderer": "2.7.13",
    "vue-template-compiler": "2.7.13",
    "vuepress": "1.9.7",
    "vuex": "3.6.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "test/e2e/specs",
    "commonPath": "test/e2e/common",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "test/e2e/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
